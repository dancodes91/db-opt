{
  "name": "zoom-kiosk",
  "version": "1.0.0",
  "description": "Zoom Kiosk Remote PC Meeting App with auto screen share and remote control",
  "main": "dist/main/index.js",
  "scripts": {
    "prebuild": "node scripts/prebuild.js",
    "build": "tsc",
    "postbuild": "node scripts/postbuild.js",
    "rebuild:native": "electron-rebuild -f --module-dir . --which-module robotjs,global-mouse-events",
    "rebuild:robotjs": "electron-rebuild -f --module-dir . --which-module robotjs",
    "rebuild:mouse": "electron-rebuild -f --module-dir . --which-module global-mouse-events",
    "start": "npm run build && electron .",
    "dev": "npm run build && electron .",
    "package": "npm run build && electron-builder --win",
    "clean": "rimraf dist build release",
    "setup:sdk": "node scripts/setup-sdk.js",
    "check:sdk": "node scripts/check-sdk.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "electron": "^39.2.2",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "global-mouse-events": "^1.2.0",
    "google-protobuf": "^3.21.4",
    "robotjs": "^0.6.0"
  },
  "build": {
    "extends": "electron-builder.json"
  }
}
